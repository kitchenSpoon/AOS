menuconfig LIB_ELFLOADER
    bool "Build elfloader"
    default y
    depends on ARCH_ARM
    select LIB_CPIO

config ARM_ERRATA_764369
    bool "Workaround for ARM errata 764369"
    depends on LIB_ELFLOADER
    default y
    help
        Work around for a Cortex-A9 errata. Derived from Linux kernel.
